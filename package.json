{
  "name": "algorithms",
  "version": "1.0.0",
  "description": "Solved algorithms and data structures problems in many languages",
  "main": "index.js",
  "repository": "git@github.com:marcosfede/algorithms.git",
  "author": "Federico Marcos <marcosfede@gmail.com>",
  "license": "GPL",
  "private": false,
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "xo": "0.18.2"
  },
  "scripts": {
    "lint": "xo",
    "fix": "xo --prettier --fix ./**/*.js"
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "extends": ["prettier"],
    "plugins": ["unicorn"],
    "rules": {
      "unicorn/filename-case": ["error", {"case": "snakeCase"}],
      "no-eval": ["off"]
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5",
    "eslintIntegration": true
  }
}
